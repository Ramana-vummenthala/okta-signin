{
    "name": "MyApp",
    "version": "0.0.1",
    "author": "Siva Kotaprolu",
    "bugs": "https://github.com/Siva Kotaprolu/MyApp/issues",
    "dependencies": {
        "@okta/okta-signin-widget": "^6.3.2",
        "compression": "^1.7.4",
        "express": "^4.18.1",
        "helmet": "^5.0.2"
    },
    "engines": {
        "node": ">=10.13.0",
        "npm": ">=6.4.1",
        "yarn": ">=1.9.4"
    },
    "homepage": "https://github.com/Siva Kotaprolu/MyApp",
    "keywords": [
        "lwc"
    ],
    "license": "MIT",
    "nodemonConfig": {
        "watch": [
            "src/server/**/*.js"
        ],
        "ext": "js",
        "ignore": [
            "src/**/*.spec.js",
            "src/**/*.test.js"
        ],
        "exec": "node ./src/server/api.js"
    },
    "repository": "Siva Kotaprolu/MyApp",
    "scripts": {
        "build": "lwc-services build -m production",
        "build:development": "lwc-services build",
        "lint": "eslint ./src/**/*.js",
        "postinstall": "husky install",
        "precommit": "npm run prettier:verify && [ $? -eq 0 ] && npm run lint",
        "prettier": "prettier --write \"**/*.{css,html,js,json,md,ts,yaml,yml}\"",
        "prettier:verify": "prettier --list-different \"**/*.{css,html,js,json,md,ts,yaml,yml}\"",
        "start": "run-p start:client start:api",
        "start:api": "node src/server/api.js",
        "start:client": "node scripts/server.js",
        "test:unit": "lwc-services test:unit",
        "test:unit:coverage": "lwc-services test:unit --coverage",
        "test:unit:debug": "lwc-services test:unit --debug",
        "test:unit:watch": "lwc-services test:unit --watch",
        "watch": "run-p watch:client watch:server",
        "watch:client": "lwc-services watch",
        "watch:server": "nodemon"
    },
    "devDependencies": {
        "@lwc/rollup-plugin": "^2.13.3",
        "@okta/oidc-middleware": "4.0.1",
        "@rollup/plugin-replace": "^4.0.0",
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "dotenv": "8.2.0",
        "eslint": "^7.32.0",
        "eslint-plugin-inclusive-language": "^2.2.0",
        "express": "~4.16.1",
        "express-session": "1.17.1",
        "http-errors": "~1.6.3",
        "husky": "^7.0.4",
        "lwc": "^2.13.3",
        "lwc-services": "^3.2.2",
        "morgan": "~1.9.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.6.2",
        "pug": "^2.0.4",
        "rollup": "^2.72.1"
    }
}
